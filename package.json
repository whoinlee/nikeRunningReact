{
  "name": "nikeRunningReact",
  "version": "0.1.0",
  "description": "A sample DataViz project with Nike+ data, built in React",
  "repository": {
    "type": "git",
    "url": "https://github.com/whoinlee/nikeRunningReact.git"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.2"
  },
  "dependencies": {
    "flot": "^0.8.0-alpha",
    "jquery": "^3.1.1",
    "leaflet": "^1.0.3",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
